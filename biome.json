{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true
	  }
	},
	"json": {
	  "parser": {
		"allowComments": true
	  }
	},
	"javascript": {
	  "formatter": {
		"quoteStyle": "single"
	  }
	},
	"formatter": {
	  "indentStyle": "space",
	  "lineWidth": 120
	}
  }
  