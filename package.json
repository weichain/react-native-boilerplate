    "babel-loader": "^8.3.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "lint-staged": ">=10",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
